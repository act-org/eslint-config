{
  "author": "Jon Cursi <jon.cursi@encoura.org>",
  "bugs": {
    "email": "jon.cursi@encoura.org",
    "url": "https://github.com/act-org/eslint-config/issues"
  },
  "contributors": [
    "Jon Cursi <jon.cursi@encoura.org>",
    "Adam Weeden <adam.weeden@encoura.org>"
  ],
  "dependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-disable": "^2.0.3",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-new-with-error": "^5.0.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "markdownlint-cli": "^0.35.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "description": "ACT's preferred configs for TypeScript, Prettier, ESLint, CommitLint, and MarkdownLint.",
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "clean-publish": "5.0.0",
    "husky": "9.1.6",
    "jest": "29.6.1",
    "lint-staged": "13.2.3",
    "npm-run-all2": "6.2.3",
    "rimraf": "6.0.1",
    "semantic-release": "24.1.1"
  },
  "homepage": "https://github.com/act-org/eslint-config#readme",
  "keywords": [
    "commitlint",
    "eslint",
    "eslint-config",
    "markdownlint",
    "prettier",
    "typescript"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js}": "npm run test:lint:js",
    "*.{md}": "npm run test:lint:md"
  },
  "main": "index.js",
  "name": "@actinc/eslint-config",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/act-org/eslint-config.git"
  },
  "scripts": {
    "build": "npm-run-all2 build:*",
    "build:clean": "rimraf dist && mkdir dist",
    "build:configs": "cp -rf commitlint.config.js index.js markdownlint.config.json prettier.config.js tsconfig.json dist",
    "build:docs": "cp -rf CHANGELOG.md LICENSE README.md dist",
    "build:package_json": "clear-package-json package.json -o dist/package.json",
    "prepare": "husky",
    "test": "npm-run-all2 --parallel test:**",
    "test:lint:js": "eslint . --ext .js --quiet",
    "test:lint:md": "markdownlint README.md --config markdownlint.config.json",
    "test:tsconfig": "tsc"
  },
  "version": "4.2.0"
}
